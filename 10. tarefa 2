#include <iostream>
#include <math.h>

using namespace std;

float coss(){
    float soma=0;
    float j, i,k,x, p;
    for(j=1; j>=1 ; j=j+2){
        y=j*(j-1)*y;
        k=1/y;
        
        soma=soma+k;
    }
    cout<<soma<<endl;
    x=soma*32.0;
    cout<<x<<endl;
    p=pow(x,1.0/3.0);
    cout<<p<<endl;
    return p;
}

int main()
{
    float x, coss, sen2, sen;
    
    cout<<"valor do angulo";
    cin>>x;
    
    sen2=(1-(coss*coss));
    sen=sqrt(sen2);
    
    cout<<"valor do seno ="<<sen;
    

    return 0;
}
