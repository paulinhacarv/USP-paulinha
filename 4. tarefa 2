#include <iostream>

using namespace std;

int main()
{
    float x,y;
    
    cout<<"valor de x"<<endl;
    cin>>x;
    cout<<"valor de y"<<endl;
    cin>>y;
    
    if(x<=0 || y<=0){
        cout<<"("<<x<<","<<y<<") nao pertence a H";
    }
    
    if((x*x)+(y*y)<1){
        cout<<"("<<x<<","<<y<<") pertence a H";
    } else
    cout<<"("<<x<<","<<y<<") nao pertence a H";
    
    return 0;
}
