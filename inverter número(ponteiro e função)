#include <stdio.h>

void inverter(int *a, int *b);


int main() {
int a, b;


    printf("digite valor de a e b\n");
    scanf("%d %d", &a, &b);

    inverter(&a, &b);
    printf("valor de a=%d, valor de b=%d", a, b);

return 0;
}

void inverter(int *a, int *b){
    int c;
    c=*a;
    *a=*b;
    *b=c;
}
