#include<stdio.h>

int main(){
float prova1, prova2, prova3, media;
    printf("\n informe a prova1");
    scanf("%f", &prova1);
    printf("\n informe a prova2");
    scanf("%f", &prova2);
    printf("\n informe a prova3");
    scanf("%f", &prova3);
    media=(prova1+prova2+prova3)/3;
    printf("\n a media = %f\n", media);
    if(media>=5){
    printf("voce foi aprovado");}
    if(media<5){
    printf("voce foi reprovado");}
}
