#include <iostream>

using namespace std;

float soma(){
    float soma=0;
    float j=1, i, k;
    for(i=1; i<=99; i=i+2){
        k=i/j;
        j=j+1;
        soma=soma+k;
    }
    return soma;
}

int main()
{
    
    cout<<" resultado de (1/1)+(3/2)+(5/3)+...+(99/50) = "<<soma();
    
    return 0;
}
