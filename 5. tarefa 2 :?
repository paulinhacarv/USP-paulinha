#include <iostream>

using namespace std;

int main()
{
    float resul=0, k;
    float n, j, i;
    
    cout<<"valor de n"<<endl;
    cin>>n;
    
    j=n;
    
    for(i=1; i<=n; i++){
        k=i/j;
        j=j-1;
        resul=resul+k;
    }
    
    cout<<" resultado = "<<resul;
    
    return 0;
}
