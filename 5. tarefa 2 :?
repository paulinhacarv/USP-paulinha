#include <iostream>

using namespace std;

int main()
{
    float resul=0, k;
    float n, j, i;
    
    cout<<"valor de n"<<endl;
    cin>>n;
    
    for(i=1; i<=n; i++){
        for(j=n; j>=1; j--){
            k=i/j;
            resul=resul+k;
        }
    }
    
    cout<<" resultado = "<<resul;
    
    return 0;
}
