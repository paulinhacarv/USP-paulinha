#include <stdio.h>
#include <math.h>
int main()
{
    double ano, bissexto1, bissexto2, bissexto3;
    
    printf("\n informe um ano");
    scanf("%lf", &ano); 
    
    bissexto1=ano%400;
    bissexto2=ano%4;
    bissexto3=ano%100;
    
    if(bissexto1==0 || bissexto2==0 && bissexto3!=0){
    printf("\n o ano e bissexto");
    }
    
    else{
        printf("\n o ano nao e bissexto");
    }
    return 0;
}




         correto
         
         
         
        #include <stdio.h>
#include <math.h>
int main()
{
    int ano;

    printf("\n informe um ano");
    scanf("%d", &ano);

    if(ano%400==0 || ano%4==0 && ano%100!=0){
    printf("\n o ano e bissexto");
    }

    else{
        printf("\n o ano nao e bissexto");
    }
    return 0;
}
