#include<stdlib.h>
#define COL 4
#define LINHA 4
#include<stdio.h>
#include<time.h>

int main(){

    int mat[LINHA][COL], linha, coluna;
    srand(time(NULL));

    for(linha=0; linha<(LINHA-1); linha++)

    for(coluna=0; coluna<COL; coluna++){

        mat[linha][coluna]=rand()%20;

        printf("\t %d", mat[linha][coluna]);

    }

        printf("\n");
        
    mat[4][1]=mat[1][1]+mat[2][1]+mat[3][1];
    mat[4][2]=mat[1][2]+mat[2][2]+mat[3][2];
    mat[4][3]=mat[1][3]+mat[2][3]+mat[3][3];
    mat[4][4]=mat[1][4]+mat[2][4]+mat[3][4];


printf("\n");


    return 0;
}





