#include <stdio.h>

int main()
{
    int x, y, z=0, w=0;

    printf("\n informe o primeiro numero(x)\t");
    scanf("%d", &x);

    printf("\n informe o segundo numero(y)\t");
    scanf("%d", &y);


        while(x<y){
            z=z+x;
            x=x+1;

    }

        w=z+y;
        printf("\n a soma de %d a %d= %d", x, y, w);

    return 0;
}
